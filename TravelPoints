package com.ladera.travelPoints;

/**
 *
 * @author Jaffy
 */
public abstract class TravelPoints {
    public String date;
    public String passengerName;
    public int passengerAge;
    public String passengerAadhar;
    public int seatNumber;
    public boolean[] bsrFreq;
    public boolean[] agsFreq;
    public boolean[] nellaiFreq;
    public boolean[] greekFreq;
    public abstract void sourceDestination();
}
